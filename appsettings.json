{
  "ConnectionStrings": {
    "MySql_ConnectionString": "server=localhost;user=ecomreadydev;password=;database=ecommercereadydev;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JwtSettings": {
    "Issuer": "AuthReadyAPI",
    "Audience": "AuthReadyAPIUsers",
    "DurationInMinutes": 10,
    "Key": "AuthReadyAPIKey"
  },
  "Serilog": {
    "MinimalLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5027"
        }
      }
    ]
  },
  "CloudinarySettings": {
    "cloud_name": "",
    "api_key": "",
    "api_secret": ""
  },
  "Stripe": {
    "PubKey": "",
    "SecretKey": ""
  },
  "AllowedHosts": "*"
}
